<!-- views/data-table.ejs -->
<%- include('layout', { title: 'Data Table' }) %>
<h1>Data Table Page</h1>

<table>
  <thead>
    <tr>
      <th>User ID</th>
      <th>Username</th>
      <th>Email</th>   
    </tr>
  </thead>
  <tbody>
    <% users.forEach(user => { %>
      <tr>
        <td><%= user._id %></td>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
      </tr>
    <% }); %>
  </tbody>
</table>
